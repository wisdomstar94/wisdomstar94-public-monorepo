{
  "name": "@wisdomstar94/vanilla-js-util",
  "version": "0.0.2",
  "license": "MIT",
  "scripts": {
    "ncu:update": "ncu -u",
    "pack:build": "vite build && tsc",
    "npm:publish": "npm publish --access=public"
  },
  "peerDependencies": {
    "react": "^18.3.1",
    "react-dom": "^18.3.1"
  },
  "devDependencies": {
    "@types/node": "^22.3.0",
    "@wisdomstar94/tsconfig-lib": "workspace:^",
    "typescript": "^5.5.4",
    "vite": "^5.4.0"
  },
  "type": "module",
  "types": "./dist/src/index.d.ts",
  "main": "./dist/index.cjs",
  "module": "./dist/index.mjs",
  "files": [
    "package.json",
    "dist/*",
    "!dist/*.tsbuildinfo"
  ],
  "exports": {
    ".": {
      "import": {
        "types": "./dist/src/index.d.ts",
        "default": "./dist/index.mjs"
      },
      "require": {
        "types": "./dist/src/index.d.ts",
        "default": "./dist/index.cjs"
      }
    }
  },
  "dependencies": {
    "ipaddr.js": "^2.2.0"
  }
}
